extends ./assets/pugs/_layout.pug

mixin list(listTitle,test)
  .col-4.ticket-table.bg-light
    h3 #{listTitle}
    div #{test}

block vars
  include ./assets/pugs/_config.pug
  - var key = "top"
  - var page = pages[key]
block header 
  .header.sticky-top
    h2 #{page.title}
    h1=message
block contents
  -var sample = "test"
    section.row.board
      +list("bb","c")
      +list("cc","d")
      +list("dd")
      +list("ff")
  include ./css/index.pug
  include ./include/_script.pug

  script.
    ticketMounted()