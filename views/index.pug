extends ./assets/pugs/_layout.pug

mixin list(listTitle,test)
  .col-4
    h3 #{listTitle}
    div #{test}

block vars
  include ./assets/pugs/_config.pug
  - var key = "top"
  - var page = pages[key]

block contents
  h2 #{page.title}
  h1=message
    section.row.board
      +list("bb","c")
      +list("cc","d")
      +list("dd")
      +list("ff")